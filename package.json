{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "Bot Discord para moderação e entretenimento",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:build": "tsc --watch",
    "clean": "rmdir /s /q dist 2>nul || echo \"Pasta dist já limpa\""
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.3.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
